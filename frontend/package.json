{
  "name": "personal-website-geni",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "typecheck": "tsc -b",
    "format": "npx prettier . --write",
    "test": "npm run test:unit",
    "test:unit": "vitest run",
    "test:e2e": "npx cypress run",
    "generate-client": "openapi-ts",
    "docker:build": "cd ../ && docker compose build",
    "docker:start": "cd ../ && docker compose up -d",
    "docker:stop": "cd ../ && docker compose down",
    "docker:dev:build": "cd ../ && docker compose -f dev.docker-compose.yaml build",
    "docker:dev:start": "cd ../ && docker compose -f dev.docker-compose.yaml up -d",
    "docker:dev:down": "cd ../ && docker compose -f dev.docker-compose.yaml down",
    "docker:dev:stop": "cd ../ && docker compose -f dev.docker-compose.yaml stop",
    "docker:dev:start:db": "cd ../ && docker compose -f dev.docker-compose.yaml up db -d",
    "docker:dev:down:db": "cd ../ && docker compose -f dev.docker-compose.yaml down db",
    "backend:format": "cd ../ && ruff format && ruff check --fix",
    "backend:prestart": "cd ../ && script/prestart.sh",
    "backend:migrate:up": "cd ../backend && alembic upgrade head",
    "backend:migrate:down": "cd ../backend && alembic downgrade base"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.0.14",
    "@tanstack/react-query": "^5.69.0",
    "@tanstack/react-router": "^1.114.17",
    "@uiw/react-md-editor": "^4.0.5",
    "flickr-justified-gallery": "^2.2.0",
    "gray-matter": "^4.0.3",
    "lightgallery": "^2.8.3",
    "lucide-react": "^0.485.0",
    "motion": "^12.6.2",
    "radix-ui": "^1.1.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwindcss": "^4.0.14"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@hey-api/openapi-ts": "^0.64.14",
    "@tanstack/eslint-plugin-query": "^5.68.0",
    "@tanstack/react-router-devtools": "^1.114.21",
    "@tanstack/router-plugin": "^1.114.17",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "cypress": "^14.2.0",
    "daisyui": "^5.0.4",
    "eslint": "^9.21.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "jsdom": "^26.0.0",
    "prettier": "3.5.3",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.24.1",
    "vite": "^6.2.0",
    "vitest": "^3.0.8"
  }
}
